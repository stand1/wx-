/* pages/enter/enter.wxss */
.wapper{
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #555555;
  width: 100%;
  box-sizing: border-box;
}
.item{
  width: 100%;
  box-sizing: border-box;
  font-size: 16px;
  color: #555555;  
  border-top:8px solid #fafafa;
}
.contenlist_item{
  width: 100%;
  height: 48px;
  line-height: 48px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
  font-size: 16px;
}
.contenlist_item_left{
  margin-left: 20px;
}
.contenlist_item_right{
  margin-right: 20px;
}
.contenlist_item_right text{
  margin-right: 10px;
  color: #9e9e9e;
}
.tubiao{
  width: 13px;
  height: 13px;
  margin-right: 10px;
}
.next{
  width: 8.5px;
  height: 8.5px;
}